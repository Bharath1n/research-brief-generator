# Example environment variables for the Research Brief Generator
# Copy this file to .env and replace with your actual API keys

# Google API Key for Gemini LLM
GOOGLE_API_KEY=your_google_api_key_here

# Tavily API Key for web search
TAVILY_API_KEY=your_tavily_api_key_here

# LangChain API Key for tracing
LANGCHAIN_API_KEY=your_langchain_api_key_here

# HuggingFace API Token (optional, removed from code but kept for completeness)
HUGGINGFACEHUB_API_TOKEN=your_hf_api_token_here

# LangChain Tracing Configuration
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=research-brief-generator